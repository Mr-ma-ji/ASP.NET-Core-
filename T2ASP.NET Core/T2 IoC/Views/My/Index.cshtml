@using T2_IoC.Model
@model List<student>
@{
    ViewData["Title"] = "Index";
}

<h1>依赖注入测试</h1>
<table class="table table-striped table-hover" >
    <thead>
		<tr>
			<th>编号</th>
			<th>姓名</th>
			<th>年龄</th>
			<th>性别</th>
		</tr>
    </thead>
	<tbody>
		@foreach (var item in Model)
		{
		<tr>
			<td>@item.stuid</td>
			<td>@item.stuname</td>
			<td>@item.stuage</td>
			<td>@item.stusex</td>
		</tr>
		}
	</tbody>
</table>

